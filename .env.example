# Copy this file to .env and fill in your actual API credentials
# DO NOT commit the .env file to version control

# Google API Key (for Drive, Sheets, and Gemini APIs)
# Get from: https://console.cloud.google.com/apis/credentials
VITE_GOOGLE_API_KEY=YOUR_GOOGLE_API_KEY_HERE

# Google OAuth 2.0 Client ID
# Get from: https://console.cloud.google.com/apis/credentials  
VITE_GOOGLE_CLIENT_ID=YOUR_GOOGLE_CLIENT_ID_HERE

# Google Drive Folder ID (root folder for stock data)
# Create a folder in Google Drive and copy its ID from the URL
VITE_GOOGLE_DRIVE_FOLDER_ID=YOUR_GOOGLE_DRIVE_FOLDER_ID_HERE

# LlamaParse API Key (starts with "llx-")
# Get from: https://cloud.llamaindex.ai/
VITE_LLAMAPARSE_API_KEY=YOUR_LLAMAPARSE_API_KEY_HERE

# Optional: Batch size for PDF processing (default: 10)
VITE_LLAMAPARSE_BATCH_SIZE=10

# Optional: Processing timeout in milliseconds (default: 5 minutes)
VITE_PROCESSING_TIMEOUT=300000